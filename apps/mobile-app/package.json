{
  "name": "mobile-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "eas-build-pre-install": "bun install && bun ../../patches/patch-packages.ts",
    "eas-build-post-install": "bun turbo run build --filter=mobile-app",
    "dev": "cli expo dev --platform ios --profile debug",
    "build:ios-debug": "cli expo build --platform ios --profile debug",
    "build:ios-release-simulator": "cli expo build --platform ios --profile release-simulator",
    "patch:ios-release-simulator": "cli expo patch --platform ios --profile release-simulator",
    "launch:ios-release-simulator": "cli expo launch --platform ios --profile release-simulator",
    "update:preview": "cli expo update --platform ios --profile release-simulator",
    "biome": "cli tools biome",
    "typecheck": "cli tools typecheck",
    "fix": "cli tools fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/native": "^6.0.2",
    "expo": "~49.0.15",
    "expo-build-properties": "~0.8.3",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.19",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "tsconfig": "workspace:*"
  },
  "private": true
}
